<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="36dp"
    android:height="18dp"
    android:viewportWidth="1200"
    android:viewportHeight="600">

    <!-- Màu Nền (Blue) -->
    <path
        android:fillColor="#012169"
        android:pathData="M0,0h1200v600H0z" />

    <!-- Chữ thập St. Andrew (Trắng - Scotland) -->
    <path
        android:fillColor="#FFFFFF"
        android:pathData="M0,0L1200,600ZM1200,0L0,600Z"
        android:strokeWidth="120"
        android:strokeColor="#FFFFFF" />

    <!-- Chữ thập St. Patrick (Đỏ - Ireland) -->
    <!-- Đây là phần phức tạp nhất, gồm 4 đường chéo nhỏ màu đỏ -->
    <group>

        <path

            android:fillColor="#C8102E"

            android:pathData="M0,0 L1200,600 M0,600 L1200,0"

            android:strokeWidth="60"

            android:strokeColor="#C8102E" />
    </group>

    <!-- Viền trắng của chữ thập St. George -->
    <path
        android:fillColor="#FFFFFF"
        android:pathData="M400,0h400v600H400z" />
    <path
        android:fillColor="#FFFFFF"
        android:pathData="M0,200h1200v200H0z" />

    <!-- Chữ thập St. George (Đỏ - England) -->
    <path
        android:fillColor="#C8102E"
        android:pathData="M480,0h240v600H480z" />
    <path
        android:fillColor="#C8102E"
        android:pathData="M0,240h1200v120H0z" />

</vector>